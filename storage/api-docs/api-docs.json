    "paths": {
        "/accommodation-draft": {
            "get": {
                "tags": [
                    "Accommodation"
                ],
                "summary": "Get accommodation draft",
                "description": "Retrieves the saved draft for an accommodation listing",
                "operationId": "getAccommodationDraft",
                "parameters": [
                    {
                        "name": "status",
                        "in": "query",
                        "description": "Filter by draft status",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "draft",
                            "enum": [
                                "draft",
                                "waiting_for_approval",
                                "published"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Accommodation draft retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AccommodationDraft"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "404": {
                        "description": "Draft not found"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Accommodation"
                ],
                "summary": "Create accommodation draft",
                "description": "Creates a new draft for an accommodation listing",
                "operationId": "createAccommodationDraft",
                "requestBody": {
                    "description": "Accommodation draft data",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "data"
                                ],
                                "properties": {
                                    "data": {
                                        "description": "The draft data for the accommodation listing",
                                        "type": "object",
                                        "example": {
                                            "title": "Cozy Apartment",
                                            "description": "A nice place to stay"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Accommodation draft created successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AccommodationDraft"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/accommodation-draft/{accommodationDraft}": {
            "put": {
                "tags": [
                    "Accommodation"
                ],
                "summary": "Update accommodation draft",
                "description": "Updates a draft for an accommodation listing",
                "operationId": "updateAccommodationDraft",
                "parameters": [
                    {
                        "name": "accommodationDraft",
                        "in": "path",
                        "description": "Accommodation draft ID",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid",
                            "example": "019a4b7b-3481-738a-a2ff-d93fc45bac01"
                        }
                    }
                ],
                "requestBody": {
                    "description": "Accommodation draft data",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "current_step",
                                    "data"
                                ],
                                "properties": {
                                    "current_step": {
                                        "description": "The current step in the accommodation listing process",
                                        "type": "integer",
                                        "example": 2
                                    },
                                    "data": {
                                        "description": "The draft data for the accommodation listing",
                                        "type": "object",
                                        "example": {
                                            "title": "Cozy Apartment",
                                            "description": "A nice place to stay"
                                        }
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Accommodation draft saved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/AccommodationDraft"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad request"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/accommodation-drafts/stats": {
            "get": {
                "tags": [
                    "Accommodation"
                ],
                "summary": "Get accommodation draft statistics",
                "description": "Retrieves statistics about accommodation drafts",
